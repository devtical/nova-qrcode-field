{
    "name": "devtical/nova-qrcode-field",
    "description": "Nova QR code field",
    "keywords": [
        "laravel",
        "nova",
        "qr",
        "qrcode"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.13"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Qrcode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Qrcode\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
